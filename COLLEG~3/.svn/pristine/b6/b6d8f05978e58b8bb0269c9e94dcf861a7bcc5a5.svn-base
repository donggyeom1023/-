.bx-wrapper .bx-pager-item{font-size:0;line-height:0}
.bx-wrapper .bx-prev{left:10px;}
.bx-wrapper .bx-prev:focus,.bx-wrapper .bx-prev:hover{background-position:0 0}
.bx-wrapper .bx-next{right:10px;}
