#wrap {
    position: relative;
    min-height: 100%;
    display: block;
    word-break: keep-all;
  }
  #header {
    width: 440px;
    height: 100px;
    margin: 0 auto;
    margin-top: 30px;
    font-size: 50px;
    text-align: center;
}
  #container {
    margin: 0;
    padding: 0;
  }
  fieldset, img {
    border: 0;
  }
  .login_form {
    text-align: center;
    width : 50%;
    margin: 0 auto;
  }
  .int {  
    height: 20px;
    width: 30%;
    padding:0;
  }
.btn {
    height: 20px;
    width: 100%;
    text-align: center; 
}

#se_1 {
    margin: 0 auto;
    font-size: 15px;
    width: 15%;
}
.pr_list_search {
    text-align: center;
    height: 20px;
}
#id, #pw {
  width: 30%;
  height: 15px;
}
#pw {
  width: 27%;
}
h1 {
  display: block;
  font-size: 2em;
  margin-block-start: 0.2em;
  margin-block-end: 0.2em;
  margin-inline-start: 0px;
  margin-inline-end: 0px;
  font-weight: bold;
}
.btLoginPC {
  position: absolute;
  top: 0;
  left: 148px;
}
.loginSec legend {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 43px;
  border-bottom: 1px solid #cbcbcb;
}
#contents {
  position: relative;
  width: 440px;
  margin: 0 auto;
  padding-bottom: 20px;
}
.loginSec {
  position: relative;
  width: 440px;
  margin: 0 auto;
}
.loginSec .idPass {
  overflow: hidden;
  padding-top: 44px;
}
a, abbr, acronym, address, big, blockquote, body, button, caption, cite, code, dd, div, dl, dt, fieldset, form, h1, h2, h3, h4, h5, h6, img, ins, kbd, label, legend, li, ol, p, pre, q, s, samp, small, span, strike, strong, table, tbody, td, tfoot, th, thead, tr, ul {
  margin: 0;
  padding: 0;
  border: 0;
  vertical-align: baseline;
  background: 0 0;
  font-size: 1em;
}
form {
  display: block;
  margin-top: 0em;
}
fieldset {
  display: block;
  margin-inline-start: 2px;
  margin-inline-end: 2px;
  padding-block-start: 0.35em;
  padding-inline-end: 0.75em;
  padding-block-end: 0.625em;
  min-inline-size: min-content;
}
body {
  width: 100%;
  height: 100%;
  overflow-y: scroll;
  font: 12px/1.335 "돋움",dotum,"Segoe WP",Helvetica,"sans serif";
  color: #666;
}
legend {
  display: block;
  padding-inline-start: 2px;
  padding-inline-end: 2px;
  border-width: initial;
  border-style: none;
  border-color: initial;
  border-image: initial;
}
.loginSec legend span {
  display: block;
  width: 138px;
  height: 28px;
  padding-top: 15px;
  color: #222;
  font-weight: 700;
  text-align: center;
  border: 1px solid #cbcbcb;
  border-bottom: 0;
  background-color: #fafafa;
}
.loginSec .id {
  position: relative;
  width: 400px;
  height: 55px;
  padding: 6px 0 0 7px;
  border: 1px solid #cbcbcb;
}
.loginSec legend {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 43px;
  border-bottom: 1px solid #cbcbcb;
}
.loginSec .input01 {
  width: 400px;
  padding: 12px 0 9px 10px;
  font-size: 16px;
  color: #222;
  font-weight: 700;
  border: 0;
  background-color: transparent;
  outline: 0;
}
.loginSec .pass {
  position: relative;
  width: 400px;
  height: 55px;
  padding: 6px 0 0 7px;
  margin-top: 1px;
  border: 1px solid #cbcbcb;
}
.loginSec .btLogin {
  margin-top: 21px;
}
.loginSec .loginMenu {
  position: relative;
  padding: 25px 0 37px;
  color: #d9d9da;
}
.btLoginID a, .btLoginPC a{
  display: block;
  width: 138px;
  height: 27px;
  padding-top: 15px;
  color: #7a7a7a;
  text-decoration: none;
  font-weight: 700;
  text-align: center;
  border: 1px solid #dedede;
  border-bottom: 0;
  background-color: #f4f4f4;
}
.loginSec .focusInput {
  background-position: 0 -98px;
}
.btn {
  width: 100%;
  padding-top: 30px;
  margin : 0 5px;
  height: 50px;
  color: #fff;
  font-size: 18px;
}