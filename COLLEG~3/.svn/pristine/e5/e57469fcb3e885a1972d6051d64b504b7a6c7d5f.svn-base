}

@media (max-width: 575.98px) {
  .courTable-responsive-sm {
    display: block;
    width: 100%;
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
  }
  .courTable-responsive-sm > .courTable-bordered {
    border: 0;
  }
}

