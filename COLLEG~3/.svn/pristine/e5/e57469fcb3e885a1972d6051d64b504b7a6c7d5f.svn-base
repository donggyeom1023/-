body, ul, li {
    margin:0;
    padding:0;
    list-style:none;
  }
  a {
    color:inherit;
    text-decoration:none;
  }
  
  body {
    height:1000px;
    word-break: keep-all;
  }
  
  .left-side-bar {
    position:fixed;
    top:0;
    left:-180px;
    width:200px;
    height:100%;
    background-color:#89cbdd;
    transition:left .3s;
  }
  .left-side-bar:hover {
    left:0;
  }
  .left-side-bar > .status-ico {
    text-align:right;
  }
  .left-side-bar > .status-ico > span:last-child {
    display:none;
  }
  .left-side-bar:hover > .status-ico > span:last-child {
    display:block;
  }
  .left-side-bar:hover > .status-ico > span:first-child {
    display:none;
  }
  
  
  .menu {
    text-align:left;
  
  }
  .menu ul {
    background-color:#89cbdd;
  }
  .menu > ul {
    display:inline-block;
    padding:0 10px;
    border-radius:5px;
  }
  .menu ul > li > a {
    display:block;
    padding:10px;
  }
  .menu ul > li:hover > a {
    background-color:black;
    color:white;
  }
  .menu .sub2 {
    display:none;    
  }
  .menu #hover:hover > .sub2{
    display: block;
  }
  
  .con {
    max-width:1000px;
    margin-left:auto;
    margin-right:auto;
  }
  
  
  .img-box > img {
    width:100%;
    display:block;
  }
  
  .img-box > img {
    width:100%;
    display:block;
  }
  
  .con {
    margin-top:40px;
    margin-bottom:40px;
  }
  
  #logo > a {
    font-size:8vw;
    font-weight:bold;
    text-transform:uppercase;
  }
  
  #logo {
    text-align:center;
    margin-bottom:2vw;
    margin-top:50px;
  }
  #logo1 > a {
    font-size:8vw;
    font-weight:bold;
    text-transform:uppercase;
  }
  
  #logo1 {
    text-align:center;
    margin-bottom:2vw;
  }
  
  /* section를 목록(list)이라고 보시면 됩니다. */
  section {
    display:block;
    text-align:center;
    font-weight:bold;
  }
  section > a {
    width:100px;
    display:inline-block;
    text-align:center;
  }
  
  section > a:hover {
    color:red;
  }
  /* section > nav를 항목(list-item)이라고 보시면 됩니다. */
  
  .menu > li {
    text-decoration:none;
  }
  
  #sec1{
    text-align:right;
  }
  