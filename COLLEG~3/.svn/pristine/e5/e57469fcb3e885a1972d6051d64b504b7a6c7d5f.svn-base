
.courTable {
  width: 100%;
  margin-top: 2rem;
  margin-bottom: 1rem;
  color: #212529;
}

.courTable th,
.courTable td {
  padding: 0.75rem;
  vertical-align: top;
  border-top: 1px solid #dee2e6;
  text-align: center;
}

.courTable thead th {
  vertical-align: bottom;
  border-bottom: 2px solid #dee2e6;
  font-size: 12px;
}

.courTable tbody + tbody {
  border-top: 2px solid #dee2e6;
}

.courTable-sm th,
.courTable-sm td {
  padding: 0.3rem;
}

.courTable-bordered {
  border: 1px solid #dee2e6;
}

.courTable-bordered th,
.courTable-bordered td {
  border: 1px solid #dee2e6;
}

.courTable-bordered thead th,
.courTable-bordered thead td {
  border-bottom-width: 2px;
}

.courTable-borderless th,
.courTable-borderless td,
.courTable-borderless thead th,
.courTable-borderless tbody + tbody {
  border: 0;
}

.courTable-striped tbody tr:nth-of-type(odd) {
  background-color: rgba(0, 0, 0, 0.05);
}

.courTable-hover tbody tr:hover {
  color: #212529;
  background-color: rgba(0, 0, 0, 0.075);
}

.courTable-primary,
.courTable-primary > th,
.courTable-primary > td {
  background-color: #b8daff;
}

.courTable-primary th,
.courTable-primary td,
.courTable-primary thead th,
.courTable-primary tbody + tbody {
  border-color: #7abaff;
}

.courTable-hover .courTable-primary:hover {
  background-color: #9fcdff;
}

.courTable-hover .courTable-primary:hover > td,
.courTable-hover .courTable-primary:hover > th {
  background-color: #9fcdff;
}

.courTable-secondary,
