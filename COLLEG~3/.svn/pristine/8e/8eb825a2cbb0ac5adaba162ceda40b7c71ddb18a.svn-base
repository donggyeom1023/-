  .select-style {
    position: absolute;
    top: 85px;
    left: 0;
    border: 1px solid #ccc;
    width: 162px;
    background-color: #cacaca ;
    overflow: hidden;
  }
  .select-style select {
    padding: 9px 8px;
    width: 100%;
    border: none;
    box-shadow: none;
    background: transparent;
    background-image: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    position: relative;
    z-index: 3;
    display: block;
  }
  .select-style select::-ms-expand {
    display: none;
  }
